@model Numeros
@{
    ViewData["Title"] = "Resultado";
}

<h1>Resultado</h1>

<div>
    <div>
        <table class="table table-striped table-dark">
            <thead >
                <tr>
                    <th>#</th>
                    <th>Numeros Ingresados</th>
                    <th>Resultado Binario</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < 10; i++)
                {
                <tr>
                    <th>@(i + 1)</th>
                    <td>@Model.Numero[i]</td>
                    <td>@ViewData["Binario " + i]</td>
                </tr>
                }
            </tbody>
           
        </table>
    </div>
</div>
